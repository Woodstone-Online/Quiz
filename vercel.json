{
  "rewrites": [
    {
      "source": "/quiz/:match*",
      "destination": "/quiz/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "trailingSlash": true,
  "regions": [
    "cdg1"
  ],
  "github": {
    "silent": true
  }
}
